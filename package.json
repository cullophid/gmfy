{
  "name": "gmfy",
  "version": "1.1.0",
  "main": "",
  "scripts": {
    "test": "oolon-livetape src/**/*.test.ls | tap-spec",
    "test:watch": "oolon-livetape src/**/*.test.ls -w | tap-spec",
    "build:ls": "browserify -t browserify-livescript --extension .ls -o public/bundle.js src/main.ls",
    "build:sass": "node-sass sass/main.scss public/bundle.css",
    "watch:ls": "watchify -d -t browserify-livescript --extension .ls -o public/bundle.js src/main.ls",
    "watch:sass": "npm run build:sass -- -w src/sass",
    "build": "npm run build:ls && npm run build:sass",
    "watch": "parallelshell 'npm run watch:ls' 'npm run watch:sass'",
    "start": "serve public",
    "dev": "parallelshell 'npm run watch' 'npm start' 'livereload public'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cullophid/gmfy.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {},
  "dependencies": {
    "axios": "^0.11.1",
    "preact": "^4.8.0",
    "ramda": "^0.21.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "browserify-livescript": "^0.2.3",
    "glob": "^7.0.3",
    "node-sass": "^3.4.2",
    "oolon-livetape": "^1.0.2",
    "parallelshell": "^2.0.0",
    "serve": "^1.4.0",
    "sinon": "^1.17.4",
    "tap-dot": "^1.0.5",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "watch": "^0.18.0"
  }
}
